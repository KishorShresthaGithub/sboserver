{
  "name": "snakebite",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development kill-port 3000 && nodemon --exec npx babel-node --trace-warnings ./src/bin/www",
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production npm run clean && babel ./src --out-dir dist --copy-files",
    "prod": "cross-env NODE_ENV=production node ./dist/bin/www.js",
    "start": "cross-env NODE_ENV=production pm2 start ./dist/bin/www.js"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "core-js": "^3.15.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto-random-string": "~3.3.1",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-validator": "^6.12.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.1.0",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.6.4",
    "sharp": "^0.28.3",
    "slugify": "^1.5.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/node": "^7.14.7",
    "@babel/preset-env": "^7.14.7",
    "eslint": "^7.30.0",
    "nodemon": "^2.0.9",
    "sequelize-cli": "^6.2.0",
    "sqlite3": "^5.0.2"
  },
  "engines": {
    "node": "15.X"
  }
}
